  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
